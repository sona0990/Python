total = 0
for i in range(1,11):
  if (i%3==0 or i%2 ==0):
    total = total + i

print(total)  
   
-----------------------------

given_lit=[1,3,-9,-4,99]
total =0
i=0
while given_lit[i] > 0:
  total =total+given_lit[i]
  i=i+1
  
print(total)  
  
  
